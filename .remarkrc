{
  "settings": {
  },
  "plugins": {
    "validate-links": {
      "repository": "git@github.com:layermanager/layman.git"
    },
    "lint-no-dead-urls": {
      "skipLocalhost": true,
      "deadOrAliveOptions": {
        "timeout": 20000,
        "maxRetries": 2
      },
      "skipUrlPatterns": [
        "https://security.stackexchange.com",
        "https://github.com/LayerManager/layman/issues/"
      ]
    }
  }
}
