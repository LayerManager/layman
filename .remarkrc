{
  "settings": {
  },
  "plugins": {
    "validate-links": {
      "repository": "git@github.com:layermanager/layman.git"
    },
    "lint-no-dead-urls": {
      "skipLocalhost": true,
      "deadOrAliveOptions": {
        "timeout": 30000,
        "maxRetries": 3
      },
      "skipUrlPatterns": [
        "https://security.stackexchange.com",
        "https://github.com/LayerManager/layman/issues/",
        "https://github.com/LayerManager/layman/blob/",
        "https://docs.geoserver.org/2.26.x/en/api/"
      ]
    }
  }
}
